{
  "name": "cron",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "serverless deploy --stage dev --verbose",
    "fun": "serverless deploy function --function Cron --stage dev --verbose",
    "inv": "serverless invoke --function Cron",
    "loc": "serverless invoke local --function Cron",
    "del": "serverless remove --stage dev --verbose",
    "off": "serverless offline start"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.165.0",
    "@aws-sdk/lib-dynamodb": "^3.165.0",
    "cheerio": "^1.0.0-rc.12",
    "crawler": "1.0.0",
    "logger": "1.0.0"
  },
  "devDependencies": {
    "serverless": "^3.22.0",
    "serverless-offline": "^10.0.0",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-monorepo": "^0.11.0"
  }
}
