{
  "name": "search",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "serverless deploy --stage dev --verbose",
    "fun": "serverless deploy function --function Search --stage dev --verbose",
    "inv": "serverless invoke --function Search",
    "loc": "serverless invoke local --function Search",
    "del": "serverless remove --stage dev --verbose",
    "off": "serverless offline start"
  },
  "devDependencies": {
    "serverless": "^3.22.0",
    "serverless-offline": "^10.0.0",
    "serverless-plugin-common-excludes": "^4.0.0",
    "serverless-plugin-monorepo": "^0.11.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.165.0",
    "@aws-sdk/lib-dynamodb": "^3.165.0",
    "express": "^4.18.1",
    "logger": "1.0.0",
    "morgan": "^1.10.0",
    "serverless-http": "^3.0.2"
  }
}
